package repeticao;

import java.util.Scanner;

public class atividade5 {

	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		int soma = 0;
        for(int i = 1; i <= 20; i++) {
            System.out.print("Informe a idade (" + i + "/20): ");
            int n = ler.nextInt();
            soma += n;
        }
        System.out.println("A soma das idades Ã©: " + soma);
        ler.close();

	}

}
